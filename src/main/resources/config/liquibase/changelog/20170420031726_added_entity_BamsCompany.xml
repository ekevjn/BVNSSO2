<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity BamsCompany.
    -->
    <changeSet id="20170420031726-1" author="jhipster">
        <createTable tableName="bams_company">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="matinh" type="varchar(10)">
                <constraints nullable="false" />
            </column>

            <column name="tentinh" type="varchar(500)">
                <constraints nullable="true" />
            </column>

            <column name="diachi" type="varchar(500)">
                <constraints nullable="true" />
            </column>

            <column name="dienthoai" type="varchar(200)">
                <constraints nullable="true" />
            </column>

            <column name="fax" type="varchar(200)">
                <constraints nullable="true" />
            </column>

            <column name="tentinh_eng" type="varchar(500)">
                <constraints nullable="true" />
            </column>

            <column name="diachi_eng" type="varchar(500)">
                <constraints nullable="true" />
            </column>

            <column name="dienthoai_eng" type="varchar(200)">
                <constraints nullable="true" />
            </column>

            <column name="fax_eng" type="varchar(20)">
                <constraints nullable="true" />
            </column>

            <column name="comp_id" type="varchar(64)">
                <constraints nullable="true" />
            </column>

            <column name="sequenced" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
