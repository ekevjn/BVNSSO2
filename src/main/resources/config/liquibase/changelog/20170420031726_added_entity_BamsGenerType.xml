<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <property name="now" value="now()" dbms="h2"/>

    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle, mssql"/>

    <!--
        Added the entity BamsGenerType.
    -->
    <changeSet id="20170420031726-1" author="jhipster">
        <createTable tableName="bams_generic_type">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="type_code" type="varchar(60)">
                <constraints nullable="false" />
            </column>

            <column name="type_name" type="varchar(150)">
                <constraints nullable="true" />
            </column>

            <column name="type_status_code" type="bit">
                <constraints nullable="true" />
            </column>

            <column name="type_created_by" type="varchar(60)">
                <constraints nullable="true" />
            </column>

            <column name="type_created_date" type="date">
                <constraints nullable="true" />
            </column>

            <column name="type_last_update_by" type="varchar(60)">
                <constraints nullable="true" />
            </column>

            <column name="type_last_update_date" type="date">
                <constraints nullable="true" />
            </column>

            <column name="type_version_id" type="double">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
